{
    "name": "razlet/kohana-dynamic-structure",
    "version": "3.2",
    "description": "Kohana 3.2 module to manage menu and articles quick and easy",
    "type": "kohana-module",
    "author": "Oleg Abrazhaev <seyferseed@mail.ru>",
    "require": {
        "tinymce": "4.0.10",
        "php": ">=5.4",
        "composer/installers": "1.*"
    },
    "autoload": {
        "classmap": ["/"]
    },
    "extra": {
        "installer-paths": {
            "vendor/kohana/modules/{$name}/": ["type:kohana-module"]
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "tinymce",
                "version": "4.0.10",
                "dist": {
                    "url": "http://download.moxiecode.com/tinymce/tinymce_4.0.10_jquery.zip",
                    "type": "zip"
                }

            }
        }
    ]

}
